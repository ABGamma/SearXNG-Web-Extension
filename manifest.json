{
  "manifest_version": 2,
  "name": "SearXNG - Privacy-search engine",
  "version": "1.3",
  "author": "Erghel",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "searxng48.webp"
  },
  "browser_action": {
    "default_title": "SearXNG"
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "64.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "SearXNG",
      "keyword": "SearXNG",
      "search_url": "https://searx.be/search?q={searchTerms}",
      "favicon_url": "searxng128.webp",
      "suggest_url": "https://searx.be/search?q={searchTerms}",
      "is_default": true
    }
  }
}